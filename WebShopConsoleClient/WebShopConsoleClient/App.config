<?xml version="1.0" encoding="utf-8" ?>
<!-- App.config -->
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<system.serviceModel>
		<bindings>
			<webHttpBinding>
				<binding name="webHttpBinding_IMyService" />
			</webHttpBinding>
		</bindings>
		<client>
			<endpoint address="http://localhost:3000" binding="webHttpBinding"
				bindingConfiguration="webHttpBinding_IMyService" contract="ServiceReference1.IService1"
				name="webHttpBinding_IMyService"
				behaviorConfiguration="webhttp"/>
		</client>
		<behaviors>
			<endpointBehaviors>
				<behavior name="webhttp">
					<webHttp/>
				</behavior>
			</endpointBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>
